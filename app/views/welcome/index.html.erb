<section>

<div class="circle">
  <a href="<%=trips_path%>">
    <div id="discover">
      <%= image_tag("map_pointer.png", class:"icon")%>
    </div>
  </a>
</div>


<% if !current_user %>
  <div class="circle">
  <%=link_to new_user_session_path do%>
    <div id="register">
      <%=image_tag("backpack.png", class:"icon")%>
    </div>
  <%end%>
  </div>

<% elsif current_user.profile.id != nil %>
  <div class="circle">
    <%=link_to profile_path(current_user.profile) do%>
      <div id="register">
        <%=image_tag("backpack.png", class:"icon")%>
      </div>
    <%end%>
  </div>
<% end %>

</section>
