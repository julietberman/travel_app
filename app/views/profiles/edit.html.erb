<div class="body">
<h3>Edit Personal Info: <%=@profile.name%></h3><br/>

<%= form_for @profile do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
<br><br>
<%= f.label :gender%>
<%= f.text_field :gender %>
<br><br>

<p>DOB (DD/MM/YYYY):
<%= f.text_field :dob %></p>
<br>
<%= f.label :address %>
<%= f.text_field :address %>
<br><br>

<%= f.label :city %>
<%= f.text_field :city %>
<br><br>

<%= f.label :state %>
<%= f.text_field :state %>
<br><br>

<%= f.label :zip %>
<%= f.text_field :zip %>
<br><br>
<%= f.label :passport %>
<%= f.text_field :passport %>
<br><br>
<%= f.label :telephone_num %>
<%= f.text_field :telephone_num %>
<br><br>
<%= f.label :email %>
<%= f.text_field :email %>
<br><br>
<%= f.label :notes %>
<%= f.text_area :notes %>
<br><br>

<%= f.submit "Update", class:"submit"%>

<% end %>

</div>
